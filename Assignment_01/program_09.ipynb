   "source": [
    "# To find SUM of digits"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "c9c0e0d1-5a08-4b09-af7d-3b74053db769",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number:  342\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of digits is 9\n"
     ]
    }
   ],
   "source": [
    "def sum_digit(x):\n",
    "    s = 0\n",
    "    while x > 0:\n",
    "        digit = x % 10\n",
    "        s += digit\n",
    "        x //= 10\n",
    "    return s;\n",
    "a = int(input(\"Enter a number: \"))\n",
    "print(\"Sum of digits is %d\" %sum_digit(a))"
   ]
