   "source": [
    "# Factorial of a nummber"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "23849e33-c44c-4377-b3c6-97aff85a04fd",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number:  4\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Factorial is 24\n"
     ]
    }
   ],
   "source": [
    "def fact(num):\n",
    "    f = 1\n",
    "    for i in range(1,num + 1):\n",
    "        f *= i\n",
    "    return f;\n",
    "a = int(input(\"Enter a number: \"))\n",
    "print(\"Factorial is %d\" %fact(a))"
   ]
