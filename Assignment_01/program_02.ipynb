    "def count(y):\n",
    "    cnt = 0\n",
    "    while y > 0:\n",
    "        digit = y % 10\n",
    "        cnt += 1\n",
    "        y //= 10 \n",
    "    print(\"Count of digits is %d\" %cnt)\n",
    "    return;\n",
    "a = int(input(\"Enter a number: \"))\n",
    "count(a)"
