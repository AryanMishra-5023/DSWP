   "source": [
    "def pattern(x):\n",
    "    for i in range(0,x + 1):\n",
    "        for j in range(x - i,0,-1):\n",
    "            print(j, end = ' ')\n",
    "        print(\"\")\n",
    "    return;\n",
    "rows = int(input(\"Enter number of rows\"))\n",
    "pattern(rows)"
   ]
