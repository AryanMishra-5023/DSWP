   "source": [
    "# Pair items with prices using tuples"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "ee2bc5cc-7caa-476d-8759-abadf134d8b6",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "List of items with prices: \n",
      "Milk  25\n",
      "Eggs  50\n",
      "Bread  20\n"
     ]
    }
   ],
   "source": [
    "lists = [(\"Milk\",25),(\"Eggs\",50),(\"Bread\",20)]\n",
    "print(\"List of items with prices: \")\n",
    "for i in lists:\n",
    "    item,price = i\n",
    "    print(f\"{item}  {price}\")"
   ]
  },
