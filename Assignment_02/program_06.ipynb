   "source": [
    "# Count how many Students scored above 75 marks"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "624a4797-8cd7-444b-ba48-0ff6a2c0fb33",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "3 Students scored above 75 marks.\n"
     ]
    }
   ],
   "source": [
    "marks = (67,88,92,74,76,55)\n",
    "cnt = 0\n",
    "for i in marks:\n",
    "    if i > 75:\n",
    "        cnt = cnt + 1\n",
    "print(\"%d Students scored above 75 marks.\" %cnt)"
   ]
