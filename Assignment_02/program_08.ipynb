   "source": [
    "# Find the Average Temperature"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "82407473-add1-4112-a2bd-a64ef2874864",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Average temperature is 30.42\n"
     ]
    }
   ],
   "source": [
    "temperatures = (29.5,30.0,32.2,31.5,28.9)\n",
    "s = 0\n",
    "for i in temperatures:\n",
    "    s = s + i\n",
    "avg = s / len(temperatures)\n",
    "print(\"Average temperature is %.2f\" %avg)"
   ]
